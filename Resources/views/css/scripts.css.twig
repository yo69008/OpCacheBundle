/**
 * Scripts
 */
#scripts div {
	margin: 6.5em 0 2em;
}
#scripts div span {
	display: block;
	padding: .5em 1em .5em 2em;
	word-break: break-all;
}
#scripts div span:nth-child(odd) {
	background: #e6e6e6;
}
#scripts div b {
	display: inline-block;
	color: #000000;
	width: 6em;
	float: right;
	text-align: right;
}
